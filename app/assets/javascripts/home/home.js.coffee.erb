define [
  "angular"
  "angular-ui-router"
], (angular) ->

  #config path for common page
  angular.module("homeModule", ["ui.router"]).config([
    "$stateProvider"
    ($stateProvider) ->
      $stateProvider.state "home",
        url: "/home"
        templateUrl: "<%= asset_path("home/home.html") %>"
        controller: "HomeController"

  ]).controller "HomeController", [
    "$scope"
    "$location"
    ($scope, $location) ->

      # initialize
      $scope.pageTitle = "dummy"
  ]

