define ["angular"], (angular) ->
  "use strict"
  angular.module("awesomeModule", []).directive "awesome", [->
    restrict: "E"
    replace: true
    templateUrl: "<%= asset_path("awesome/awesome.tpl.html") %>"
    scope: {}
    link: (scope, element, attrs) ->
      scope.directiveTitle = "dummy"
      return
  ]
  return

